<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.Material3.Light">
        <!-- White/Black -->
        <color name="colorOnPrimary">@color/white</color>
        <color name="android:colorBackground">@color/white</color>
        <color name="colorSurface">@color/white</color>
        <!-- Errors -->
        <color name="colorOnError">@color/white</color>
        <color name="colorErrorContainer">@color/red1</color>
        <color name="colorError">@color/red6</color>
        <color name="colorOnErrorContainer">@color/red9</color>
        <!-- Grays -->
        <color name="colorSurfaceVariant">@color/gray3</color>
        <color name="colorOutline">@color/gray5</color>
        <color name="colorOnSurfaceVariant">@color/gray7</color>
        <!-- Setting Blue as the default in a way -->
        <color name="colorPrimaryContainer">@color/blue1</color>
        <color name="colorOnSurfaceInverse">@color/blue1</color>
        <color name="colorPrimaryInverse">@color/blue2</color>
        <color name="colorPrimary">@color/blue6</color>
        <color name="colorSurfaceInverse">@color/blue8</color>
        <color name="colorOnPrimaryContainer">@color/blue9</color>
        <color name="colorOnBackground">@color/blue9</color>
        <color name="colorOnSurface">@color/blue9</color>
    </style>
