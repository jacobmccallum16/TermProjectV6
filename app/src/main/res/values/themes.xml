<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.TermProjectV6" parent="Theme.Material3.Light">
        <!-- Error = DeleteAllBtns -->
        <item name="colorError">@color/red5</item>          <item name="colorOnError">@color/white</item>
        <!-- Background = allBackgrounds -->
        <item name="android:colorBackground">@color/white</item>
        <!-- Surface = topBar -->
        <item name="colorSurface">@color/blue1</item>          <item name="colorOnSurface">@color/blue8</item>
        <!-- PrimarySurface = cardView -->
        <item name="colorPrimarySurface">@color/blue2</item>   <item name="colorOnPrimarySurface">@color/blue9</item>
        <!-- submitBtns -->
        <item name="colorPrimary">@color/blue5</item>          <item name="colorOnPrimary">@color/white</item>
        <!-- navBtns -->
        <item name="colorPrimaryVariant">@color/blue6</item>
        <!-- navbarBackground -->
        <item name="colorPrimaryDark">@color/blue9</item>
        <!-- removeLastEntryBtn -->
        <item name="colorSecondary">@color/blue7</item>        <item name="colorOnSecondary">@color/blue1</item>
        <!-- colorTertiary =  -->
        <item name="colorTertiary">@color/gray7</item>         <item name="colorOnTertiary">@color/gray2</item>
        <!-- textColorHint =  -->
        <item name="android:textColorHint">@color/gray5</item>
        <!-- textColorTertiary = tvFeedback -->
        <item name="android:textColorTertiary">@color/gray7</item>
    </style>

